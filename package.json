{
  "name": "textmate",
  "displayName": "TextMate Keymap and features",
  "description": "TextMate features and keybindings for Visual Studio Code",
  "repository": {
    "type": "git",
    "url": "https://github.com/elia/vscode-textmate.git"
  },
  "publisher": "elia",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Keymaps"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "TextMate Settings",
      "properties": {
        "tmcode.windowTitle": {
          "type": "string",
          "default": null,
          "description": "The window title to use for the editor. If not set, the default title will be used.",
          "scope": "resource"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
